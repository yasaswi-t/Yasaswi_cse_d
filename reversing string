/*write a program to modify an array by reversing the binarized integer values and converting them back to decimal*/
int main()
{
int i,a[20],rem=0,x=0,n,b;
scanf("%d",&n);
for(i=0;i<n;i++)
scanf("%d",&a[i]);
for(i=0;i<n;i++)
{
b=a[i];
x=0;
rem=0;
while(b>0);
{
rem=b%2;
b=b/2;
x=x*2+rem;
}
printf("%d ",x);
}
}
